    <test name="OrangeHRM Info">
        <classes>
            <class name="org.akshay.OrangeHrmTest">
                <methods>
                    <include name="launchApp"/>
                    <include name="adminLogin"/>
                    <include name="NavToMyInfo"/>
                    <include name="verifyMyInfo"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>